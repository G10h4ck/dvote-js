{
  "name": "dvote-js",
  "version": "1.14.0",
  "description": "Javascript/Typescript library to interact with Vocdoni voting processes",
  "main": "dist/index",
  "types": "dist/index",
  "repository": "https://github.com/vocdoni/dvote-js.git",
  "author": "Vocdoni <root@vocdoni.io>",
  "license": "GPL-3.0-or-later",
  "private": false,
  "scripts": {
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist",
    "prebuild": "git submodule init && git submodule update",
    "build": "npm run clean && ./node_modules/.bin/tsc",
    "watch": "./node_modules/.bin/tsc -w -p .",
    "ts-node": "./node_modules/.bin/ts-node",
    "postinstall": "./scripts/post-install.sh"
  },
  "dependencies": {
    "@vocdoni/census": "file:./packages/census/vocdoni-census-1.14.1.tgz",
    "@vocdoni/client": "file:./packages/client/vocdoni-client-1.14.2.tgz",
    "@vocdoni/common": "file:packages/common/vocdoni-common-1.14.1.tgz",
    "@vocdoni/contract-wrappers": "file:./packages/contract-wrappers/vocdoni-contract-wrappers-1.14.1.tgz",
    "@vocdoni/data-models": "file:./packages/data-models/vocdoni-data-models-1.14.2.tgz",
    "@vocdoni/encryption": "file:./packages/encryption/vocdoni-encryption-1.14.1.tgz",
    "@vocdoni/hashing": "file:./packages/hashing/vocdoni-hashing-1.14.0.tgz",
    "@vocdoni/signing": "file:./packages/signing/vocdoni-signing-1.14.0.tgz",
    "@vocdoni/voting": "file:packages/voting/vocdoni-voting-1.14.0.tgz",
    "@vocdoni/wallets": "file:./packages/wallets/vocdoni-wallets-1.14.1.tgz"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  }
}
